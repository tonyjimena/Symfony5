{% extends 'base.html.twig' %}

{% block title %}Edit Projects{% endblock %}

{% block body %}
<main role="main">
  <section class="jumbotron text-center jumboindex"  style="background: url('{{ asset('uploads/images/') }}{{ projects.image }}') no-repeat center;">
    <div class="blure2">
      <div class="center">
        {{ form_start(form) }}
        <div class="form-group">
            {{ form_row(form.name, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.info, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.place, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-group">
            {{ form_row(form.image, {'attr': {'class': 'form-control'}}) }}
        </div>
        <div class="form-check">
          <input type="checkbox" class="form-check-input" id="exampleCheck1">
          <label class="form-check-label" for="exampleCheck1">Check me out</label>
        </div>
          {{ form_row(form.save, {'attr': {'class': 'btn btn-primary'}}) }}
        {{ form_end(form) }}
      </div>
    </div>
  </section>
  
</main>
{% endblock %}
